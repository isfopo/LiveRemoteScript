{
  "name": "liveremotescript",
  "version": "1.0.0",
  "scripts": {
    "install-script": "python3 install.py --name LiveRemote",
    "watch": "python3 watch.py --version 'Live 11.1.1'",
    "close-set": "pkill -x Ableton Live 11 Suite",
    "open-set": "open set/oscdevset.als",
    "reload": "npm run install-script && npm run close-set && npm run open-set"
  },
